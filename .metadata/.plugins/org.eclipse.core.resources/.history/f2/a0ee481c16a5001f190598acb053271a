# Server configuration

spring.application.name=deviuco
spring.cloud.azure.keyvault.secret.property-sources[0].endpoint=https://devi2024.vault.azure.net/
azure.keyvault.url=https://devi2024.vault.azure.net/


# JPA properties
spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=none
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
spring.mailersend.api-key=${mailsenderapi1}
spring.mailersend.email=${deviucoemail}
spring.mailersend.destination.email=${email}
spring.data.redis.url=${catalogo}

spring.datasource.url=jdbc:postgresql://localhost:5432/deviuco_usuarios
spring.datasource.username=postgres
spring.datasource.password=0000
spring.datasource.driver-class-name=org.postgresql.Driver
